<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="tf_header">
    <default-activity>header</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>languageBean</managed-bean-name>
      <managed-bean-class>com.pocapp.enoro.view.beans.LanguageBean</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <view id="header">
      <page>/fragments/tf_header/header.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
